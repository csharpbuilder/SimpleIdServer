{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ForceHttps": false,
  "IsRealmEnabled": true,
  "IdserverBaseUrl": "ISSUER-URL",
  "ScimBaseUrl": "SCIM-URL",
  "ClientId": "SIDS-manager",
  "ClientSecret": "password",
  "Scopes": [
    "openid",
    "profile",
    "role"
  ],
  "IgnoreCertificate": true,
  "DetailedErrors": true,
  "OpenTelemetryOptions": {
    "EnableOtpExported": false,
    "EnableConsoleExporter": false
  }
}